<%= form_for(@article) do |f| %>
  <div class="form-control">
    <%= f.label :title%>
    <%= f.text_field :title %>
  </div>
  <div class="form-control">
    <%= f.label :content%>
    <%= f.text_field :content %>
  </div>
  <div class="form-control">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Show', article_path(@article) %>
<%= link_to 'Back', articles_path %>
